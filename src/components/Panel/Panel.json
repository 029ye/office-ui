{
  "name": "Panel",
  "notes": "",
  "description": "Containers used in experiences that do not require explicit context for heavy-weight creation/edit/management tasks such as settings, multi-field forms, and permissions. For containers used for complex tasks that requires context, use a separate Pane alongside the existing experience such as a List/Details layout.",
  "template": "Panel.html",
  "class": "ms-Panel",
  "dependencies": [
    "CommandBar",
    "Label",
    "Overlay",
    "Pivot",
    "CommandButton",
    "ContextualMenu",
    "Button",
    "SearchBox",
    "ContextualHost",
    "PanelHost" 
  ],
  "wrapBranches": true,
  "fileOrder": [
    "Panel.html",
    "Panel.Large.html",
    "Panel.Medium.html",
    "Panel.ExtraLarge.html"
  ],
  "props": {
    "headerText": "Panel",
    "content": "Content goes here",
    "commandBarProps": {
        "commands": [
          {
            "component": "CommandButton",
            "props":  {
              "label": "Save",
              "icon": "save",
              "tag": "button",
              "iconColor": "themePrimary"
            }
          },
          {
            "component": "CommandButton",
            "props":  {
              "label": "Cancel",
              "icon": "x",
              "tag": "button",
              "iconColor": "themePrimary"
            }
          }
        ]
    }
  },
  "propsMedium": {
    "headerText": "Medium Panel",
    "content": "Content goes here",
    "modifier": "md"
  },
  "propsLarge": {
    "headerText": "Large panel",
    "content": "Content goes here",
    "modifier": "lg"
  },
  "propsExtraLarge": {
    "headerText": "Extra large panel",
    "content": "Content goes here",
    "modifier": "xl"
  },
   "propsXXL": {
    "headerText": "XXL Panel",
    "content": "Content goes here",
    "modifier": "xxl"
  }
}