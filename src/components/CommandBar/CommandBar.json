{
  "name": "CommandBar",
  "notes": "",
  "description": "A commanding surface for panels, pages, and applications. Unlike the {0} NavBar {1}, this component is intended for action and not navigation.",
  "accessibilityNotes": 
    {
      "role": "CommandBar",
      "narration": "Read all the elements in the dialog on open.",
      "keyboard": "Use the TAB key to navigate between elements in the dialog. To exit, use ESC. Trap focus in the dialog until the user dismisses it. The user needs to return to the previous element on close."
    },
  "class": "ms-CommandBar",
  "dependencies": [
    "ContextualMenu"
  ],
  "data": {
    "hasSearch": true,
    "commands":  [
      {
        "icon": "ms-Icon--start",
        "commandText": "New",
        "hasDropdown": false,
        "hasTextOnly": false
      },
      {
        "icon": "ms-Icon--x",
        "commandText": "Delete",
        "hasDropdown": false,
        "hasTextOnly": false
      },
      {
        "icon": "ms-Icon--mailDown",
        "commandText": "Archive",
        "hasDropdown": false,
        "hasTextOnly": false
      },
      {
        "icon": "ms-Icon--trash",
        "commandText": "Junk",
        "hasDropdown": false,
        "hasTextOnly": true
      },
      {
        "icon": "ms-Icon--arrowRight",
        "commandText": "Move To",
        "hasDropdown": true,
        "hasTextOnly": true
      },
       {
        "icon": "ms-Icon--dropdown",
        "commandText": "Categories",
        "hasDropdown": true,
        "hasTextOnly": true
      }
    ],
    
    "sideCommands": [
       {
        "icon": "ms-Icon--reactivate",
        "commandText": "Undo",
        "hasDropdown": true,
        "textOnly": false
      }
    ]
  },
  "branches": [
    {
      "name": "Default",
      "default": true,
      "template": "CommandBar.html",
      "branches": [
        {
          "name": "Standard",
          "default": true
        },
        {
          "name": "Close button",
          "class": "ms-Dialog--close"
        }
      ]
    }
  ]
}
