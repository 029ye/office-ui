<div class="ms-ChoiceFieldGroup">
  <div class="ms-ChoiceFieldGroup-title">
    <label class="ms-Label {{#if props.required}} is-required {{/if}}">{{props.title}}</label>
  </div>
  {{#each props.fields}}
    {{> (lookup . 'component') props=props}}
  {{/each}}
</div> 